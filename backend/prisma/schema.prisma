generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Menus {
  id           Int      @id @default(autoincrement())
  createdAt    DateTime @default(now())
  namaproduk   String
  hargaproduk  String
  deskripsi    String
  gambarproduk String
  url          String
}

